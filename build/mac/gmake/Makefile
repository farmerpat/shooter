# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=debug32
endif
export config

PROJECTS := shooter

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

shooter: 
	@echo "==== Building shooter ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f shooter.make

clean:
	@${MAKE} --no-print-directory -C . -f shooter.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug32"
	@echo "   profile32"
	@echo "   release32"
	@echo "   debug64"
	@echo "   profile64"
	@echo "   release64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   shooter"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
